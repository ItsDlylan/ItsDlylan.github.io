  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  },
  "bugs": {
    "url": "https://github.com/ItsDlylan/ItsDlylan.github.io/issues"
  },
  "homepage": "https://github.com/ItsDlylan/ItsDlylan.github.io#readme"
